.input-container {
    padding: 0rpx;
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.top-icon-car {
    width:100%;
    height: 300rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
}

.top-icon-car image {
    width: 151rpx;
    height: 111rpx;
}

.top-icon-car text {
    margin-top:33rpx;
    color: #333333;
    text-align: center;
    font-size: 36rpx;
    font-weight: 500;
}

.input-container-top {
    padding: 0;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.input-border {
    display: flex;
    flex-direction: row;
    align-items: center;
    border: 2rpx #ccc solid;
    width: 64rpx;
    height: 80rpx;
    margin-left: 20rpx;
    justify-content: center;
}

.input-border-new {
    display: flex;
    flex-direction: row;
    align-items: center;
    width: 64rpx;
    height: 80rpx;
    justify-content: center;
}

.input-border-hasdata {
    display: flex;
    flex-direction: row;
    align-items: center;
    border: 2rpx #ccc solid;
    width: 64rpx;
    height: 80rpx;
    margin-left: 20rpx;
    justify-content: center;
}

.input-line-hasdata {
    background-color: #ccc;
    width: 2rpx;
    height: 70rpx;
}

.input-line {
    background-color: #ccc;
    width: 2rpx;
    height: 70rpx;
}

.input-top-car-input {
    width: 68rpx;
    height: 70rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.input-border-new text {
    color: #000;
    text-align: center;
    font-weight: 500;
}

.input-border-hasdata text {
    font-size: 40rpx;
    color: #000;
    font-weight: 500;
}

.input-last {
    display: flex;
    width: 64rpx;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    margin-left: 20rpx;
    position: relative;
}

.input-new {
    position: absolute;
    left: 20rpx;
    top: -20rpx;
    width: 60rpx;
    height: 36rpx;
    background-color: #acd598;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    border-radius: 4rpx;
}

.input-new text {
    font-size: 16rpx;
    color: #fff;
    font-weight: 500;
}
