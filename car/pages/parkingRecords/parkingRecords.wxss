page {
    background: #F2F3F7;
}

.record_container{
    display: flex;
    flex-direction: column;
    align-items: center;
}

.top{
    height: 120rpx;
    width: 750rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    background: white;
}

.top_record{
    color: #000;
    font-size: 30rpx;
    margin-right: auto;
    margin-left: 32rpx;
    font-weight: 500;
}
.top_record text{
    color:#EE4C75;
}
.top-time{
    margin-right: 32rpx;
    width: 240rpx;
    height: 88rpx;
    background: #F5F5F5;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.top-time image{
    width: 32rpx;
    height: 32rpx;
    margin-right: 16rpx;
}

.top-time text{
    font-size: 30rpx;
    color: #000;
    font-weight: 500;
}

.line {
    width: 100%;
    height: 20rpx;
    background: #F2F3F7;
}

.list {
    display: flex;
    flex-direction: column;
}


.cell {
    display: flex;
    flex-direction: column;
    justify-content: center;
    background: white;
    width: 710rpx;
    height: 190rpx;
    padding: 0 40rpx;
    box-sizing: border-box;
    border-bottom: 20rpx #F2F3F7 solid;
    /* box-shadow: 0 4rpx 8rpx 0 rgba(204, 204, 204, 0.5); */
}

.cell-line {
    width: 100%;
}
.line1 {
    color: #333;
    display: flex;
    justify-content: space-between;
}
.carNo {
    color:rgba(51,51,51,1);
    font-size: 36rpx;
    font-weight: 500;
}
.money {
    color:rgba(51,51,51,1);
    font-size: 36rpx;
    font-weight: 500;
}
.line2 {
    margin-top: 20rpx;
    display: flex;
    justify-content: space-between;
}

.paring-duration {
    display: flex;
    justify-content: flex-end;
}

.line2 text{
    font-size: 30rpx;
    color: #999;
}