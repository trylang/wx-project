/* page/index.wxss */

page {
    background-color: #fff;
}

.topColor {
    background-color: #ee4c75;
    height: 200rpx;
    position: relative;
}

.head {
    width: 680rpx;
    height: 320rpx;
    border-radius: 20rpx;
    background-color: #fff;
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
    padding: 28rpx 29rpx 20rpx 32rpx;
    box-sizing: border-box;
    position: absolute;
    top: 0;
    left: 36rpx;
}

.banner {
    width: 750rpx;
    height: 250rpx;
    position: relative;
    background: white;
    margin-bottom: 16rpx;
}

.banner-bg {
    width: 100%;
    height: 100%;
}

.banner-head {
    /* margin-top: 22rpx; */
    position: absolute;
    width: 100%;
    top: 40rpx;
    left: 0;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.head-image {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
}

.banner-head text {
    margin-top: 16rpx;
    color: #333;
    font-size: 30rpx;
}

.banner-head-login {
    position: absolute;
    width: 100%;
    top: 40rpx;
    left: 0;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.model {
    color: #000;
}

.modal-head {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.modal-head text {
    color: #333;
    font-size: 28rpx;
    font-weight: 500;
    line-height: 40rpx;
    margin-left: 15rpx;
}

.p_icon {
    width: 33rpx;
    height: 33rpx;
}

/* .cost{
    width: 33rpx;
    height: 33rpx;
    margin-left:auto;
} */

.sur_num {
    text-align: center;
    color: #faba00;
    font-size: 72rpx;
    font-weight: 500;
    padding: 0 32rpx;
}

.num {
    text-align: center;
    color: #999;
    font-size: 28rpx;
    font-weight: 500;
}

.total {
    font-size: 28rpx;
    text-align: center;
    padding: 15rpx;
}

.modal-foot {
    display: flex;
    justify-content: center;
    padding: 32rpx 0;
    white-space: nowrap;
}

.modal-foot .modal {
    display: inline-block;
    padding: 0 20rpx;
    border-right: #ccc solid 2rpx;
}

.modal-foot .modal:last-child {
    border-right: 0;
}

.modal-foot-left, .modal-foot-right {
    flex: 0.3;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.modal-foot-left {
    border-right: #ccc solid 2rpx;
}

.floor {
    color: #333;
    font-size: 28rpx;
    margin-right: 15rpx;
}

.f_num {
    color: #faba00;
    font-weight: 600;
    font-size: 36rpx;
}

.line {
    width: 100%;
    height: 140rpx;
    background: #f5f5f5;
}

.body {
    background-color: #fff;
}

.body_head {
    display: flex;
    align-items: center;
    padding: 36rpx 40rpx;
    border-bottom: #eee solid 2rpx;
}

.c_icon {
    width: 34rpx;
    height: 34rpx;
    margin-right: 16rpx;
}

.cl_icon {
    width: 34rpx;
    height: 34rpx;
    margin-right: 16rpx;
}

.my_car {
    color: #333;
    font-size: 28rpx;
    font-weight: 500;
}

/* .car_pay{
    color:#999999;
    font-size:24rpx;
    font-weight:400;
    margin-left:auto;
} */

.max {
    color: #999;
    font-size: 24rpx;
    margin-left: auto;
}

.pic {
    display: flex;
    align-items: center;
    flex-direction: column;
}

.pic image {
    width: 120rpx;
    height: 88rpx;
    margin: 46rpx 0 24rpx 0;
}

.pic text {
    color: #ccc;
    font-weight: 500;
    font-size: 28rpx;
    margin-bottom: 18rpx;
}

.body_mid {
    display: flex;
    align-items: center;
    padding: 30rpx 40rpx;
    border-bottom: #eee solid 2rpx;
}

.car_num {
    color: #000;
    font-weight: 500;
    font-size: 36rpx;
}

.btn_con {
    margin-left: auto;
}

.btn_con text {
    border-radius: 6rpx;
    background-color: #fff;
    font-size: 30rpx;
    padding: 8rpx 28rpx 10rpx 28rpx;
}

.btn_con text::after {
    border: none;
}

.btn_con .relieve {
    color: #ef7f1b;
    border: #ef7f1b solid 2rpx;
    border-radius: 50rpx;
    line-height: 1.2;
    font-weight: 500;
    margin-right: 20rpx;
}

.btn_con .play {
    color: #ee4c75;
    border: #ee4c75 solid 2rpx;
    border-radius: 50rpx;
    line-height: 1.2;
    font-weight: 500;
}

.body_foot {
    display: flex;
    justify-content: center;
    align-items: center;
}

.plate_car {
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 30rpx;
}

.add_car {
    width: 304rpx;
    height: 88rpx;
    color:#fff;
    font-size: 32rpx;
    font-weight: 500;
    -moz-box-shadow: 0px 4px 8px #f5f5f5;
    -webkit-box-shadow: 0px 4px 8px #f5f5f5;
    box-shadow: 0px 4px 8px #f5f5f5;
    border-radius: 44rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.back{
    background: #FABA00;
}

.back1{
    background: #EE4C75;
    margin-left: 62rpx;
}

.line_one {
    width: 100%;
    height: 16rpx;
    background: #f5f5f5;
}

.foot {
    background-color: #fff;
}

.foot_head {
    display: flex;
    align-items: center;
    padding: 36rpx 40rpx;
    border-bottom: #eee solid 2rpx;
}

.r_icon {
    width: 14rpx;
    height: 24rpx;
    padding-left: 16rpx;
    margin-left: auto;
}

.top-icon-car {
    display: flex;
    position: relative;
}

.top-icon-car image {
    width: 750rpx;
    height: 304rpx;
}

.top-icon-car text {
    position: absolute;
    left: 50%;
    transform: translate(-50%, 0);
    bottom: 38rpx;
    color: #ccc;
    text-align: center;
    font-size: 30rpx;
    font-weight: 500;
}
