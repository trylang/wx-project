
  .wrap {
    position: fixed;
    width: 100%;
    height: 100%;
  }
  .cover {
    background: #000;
    opacity: .5;
    position: fixed;
    z-index: 998;
    top: 0;
    left: 0;
    overflow: hidden;
    width: 100%;
    height: 100%;
  }
  .dialog-content {
    width: 560rpx;
    position: fixed;
    background: #fff;
    text-align: center;
    top: 30%;
    left: 50%;
    margin-left: -280rpx;
    z-index: 999;
    border-radius: 30rpx;
    padding-bottom: 80rpx;
  }
  .icon{
      position: absolute;
      width: 158rpx;
      height: 222rpx;
      left: 50%;
      margin-left: -79rpx;
      z-index: 9999;
      top: -111rpx;
  }
  .dialog-close {
    position: absolute;
    right: 5px;
    top: 5px;
    width: 20px;
    height: 20px;
    text-align: center;
    cursor: pointer;
  }
  .dialog-close:hover {
    color: #4fc08d;
  }
  .title{
      font-size: 40rpx;
      color: #333;
      font-weight: bold;
      margin-top: 176rpx;
  }
  .content{
      font-size: 28rpx;
      color: #333333;
      line-height: 56rpx;
  }
 .market_name{
    width: 202rpx;
    height: 76rpx;
    background: #ff6559;
    color: #fff;
    font-size: 28rpx;
    line-height: 76rpx;
    border-radius: 38rpx;
    margin-top: 40rpx;
}