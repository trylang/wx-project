/* pages/components/tdlist/tdlist.wxss */

page .body::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
page{
	overflow-x:hidden;
}
page .scroll-fixed{
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
}

.body{
	width: 100%;
	background-color:#f5f5f5;
	font-family:PingFangSC;
	box-sizing: border-box;
	font-size: 24rpx;
}
