.content{
	overflow: hidden;
	margin: 0rpx auto 32rpx;
	width: 700rpx;
}
.outer_sphere{
	float: right;
}
.tdtt_content{
	background: #fff;
	width: 334rpx;
	margin: 16rpx 8rpx;
}
.content image{
	width: 334rpx;
}
.sotreTitle{
	padding: 16rpx;
}
.content-bottom{
	display: flex;
	justify-content: space-between;
	padding:0 16rpx;
	height: 44rpx;
	line-height: 44rpx;
	box-sizing: border-box;
}
.text_ellipsis{
	flex: 1;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap; 
}
.browse-con{
	display: flex;
	color: #999;
	font-size: 24rpx;
	height: 44rpx;
	line-height: 44rpx;
	justify-content:flex-end;
	width: 40%;
}
.browse-con image{
	width:30rpx;
	height: 20rpx;
}
