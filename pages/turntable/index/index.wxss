}
.canvasI{
  width: 600rpx;
  height: 600rpx;
  display: block;
  position: absolute;
  left: 0;
  right: 0;
  margin: auto auto;
 
  -webkit-transition:all 3s ease;
    /* Firefox 4 */
  -moz-transition:all 3s ease;
    /* Opera */
  -o-transition:all 3s ease;
   transition:all 3s ease;
}
.canvasI-image{
  width: 456rpx;
  height: 456rpx;
  display: block;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 10;
  margin: auto auto;
  
  /* -webkit-transition:all 3s ease;
  -moz-transition:all 3s ease;
  -o-transition:all 3s ease;
  transition:all 3s ease; */
}
