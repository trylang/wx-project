.all-logo .logo-fff {
    border: 2rpx solid #000;
    background: #fff;
    box-sizing: border-box;
}

.all-title {
    display: flex;
    font-size: 36rpx;
    color: #000;
    font-weight: 500;
    padding: 32rpx 32rpx 0;
    height: 44rpx;
    line-height: 44rpx;
}

.navigator-brand-list {
    height: 650rpx;
}

.integral_list {
    flex: auto;
    position: relative;
    /* margin-top: 178rpx; */
}

.integral_list scroll-view {
    position: absolute;
    /* bottom: 0;
    left: 0;
    right: 0;
    top: 0; */
}

.list_item {
    width: 50%;
    display: inline-block;
    position: relative;
    padding: 30rpx 18rpx;
    box-sizing: border-box;
    border-bottom: 1rpx solid #e5e5e5;
}

.list_item:nth-of-type(n) {
    border-right: 1rpx solid #e5e5e5;
}

.list_item image {
    width: 340rpx;
    height: 340rpx;
    display: block;
}

.list_item .title {
    color: #000;
    font-size: 30rpx;
    margin: 20rpx 0;
}

.list_item .integralNum {
    color: #999;
    font-size: 24rpx;
}

.list_item .integralNum text {
    color: #f21b7b;
}

.list_item .collection {
    color: #999;
    font-size: 20rpx;
    margin-top: 10rpx;
}

.integral_list .noData {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    overflow: hidden;
    position: relative;
    margin-top: 244rpx;
}

.noData image {
    width: 192rpx;
    height: 262rpx;
}

.noData text {
    color: #000;
    font-size: 30rpx;
    margin-top: 20rpx;
}

.myCollection_center {
    position: fixed;
    bottom: 200rpx;
    right: 20rpx;
    z-index: 50;
}

.myCollection_side {
    position: fixed;
    bottom: 200rpx;
    right: 15rpx;
    z-index: 1;
    transform: translate(50%, 0);
    opacity: 0.5;
}

.myCollection image {
    width: 94rpx;
    height: 94rpx;
    display: block;
}

.go-home {
    position: fixed;
    right: 15rpx;
    top: 60%;
    width: 100rpx;
    height: 103rpx;
}

.go-home-hide {
    position: fixed;
    right: 0;
    top: 60%;
    transform: translate(50%, 0);
    width: 100rpx;
    height: 103rpx;
    opacity: 0.5;
}

@keyframes dialogSlipToUp {
    0% {
        top: 100%;
        opacity: 0.3;
    }

    100% {
        top: 88%;
        opacity: 1;
    }
}

.show {
    visibility: visible;
    animation: dialogSlipToUp 0.3s linear both;
}
