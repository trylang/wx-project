/* rtmap/integral_mall/pages/index/index.wxss */

.integral_header {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 101;
    background: #fff;
}

.integral_top {
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    position: relative;
    margin-bottom: 30rpx;
}

.integral_top image {
    width: 24rpx;
    height: 36rpx;
}

.integral_top text {
    font-size: 60rpx;
    color: #000;
}

.integral_top .exchange_record {
    position: absolute;
    top: 10rpx;
    right: 0;
    display: inline-block;
    font-size: 20rpx;
    color: #000;
    width: 134rpx;
    height: 40rpx;
    line-height: 40rpx;
    background-color: #eee;
    border-top-left-radius: 40rpx;
    border-bottom-left-radius: 40rpx;
}

/* *index.wxss* */
@import "../../../../pages/components/band_con/index.wxss";

page .body::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

page {
    display: flex;
    flex-direction: column;
    overflow-x: hidden;
    height: 100%;
}

page .scroll-fixed {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
}

.body {
    width: 100%;
    background-color: #fff;
    font-family: PingFangSC;
    box-sizing: border-box;
    padding-top: 178rpx;
}

.integral_nav {
    display: -webkit-flex;
    flex-direction: row;
    height: 88rpx;
    width: 750rpx;
    font-size: 30rpx;
    border-top: 2rpx solid #eee;
    border-bottom: 2rpx solid #eee;
    box-sizing: border-box;
}

.integral_nav view {
    text-align: center;
    flex: 1;
    line-height: 88rpx;
    /* font-weight: bold; */
}

.bor-con {
    color: #999;
}

.conHide {
    display: none;
}

.search-history {
    color: #999;
    font-size: 30rpx;
    margin: 32rpx;
}

.history-cons {
    display: -webkit-flex;
    white-space: normal;
    word-wrap: break-word;
    width: 750rpx;
    padding: 16rpx;
}

.con-history {
    background-color: #f5f5f5;
    padding: 16rpx 32rpx;
    white-space: nowrap;
    font-size: 24rpx;
    margin: 16rpx;
}

.navigator-brand {
    display: -webkit-flex;
    flex-direction: column;
    width: 100%;
    z-index: 10;
    position: absolute;
    top: 183rpx;
    /* height: 600rpx; */
    overflow: auto;
    white-space: nowrap;
}

.navigator-brand .navigator-content {
    font-size: 30rpx;
    padding: 32rpx 0;
    text-align: center;
    background-color: #fff;
    border-bottom: 2rpx solid rgba(238, 238, 238, 1);
}

.body .navigator-name {
    color: #f21b7b;
}

.navigator-content {
    color: #000;
}

.opacityNavigator {
    position: absolute;
    top: 239rpx;
    background: rgba(0, 0, 0, 1);
    opacity: 0.5;
    z-index: 2;
    width: 100%;
    min-height: 1800rpx;
}

.triangleParent {
    position: relative;
}

.top_title {
    width: 150rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-left: 32rpx;
}

.triangle {
    height: 0;
    width: 0;
    border-bottom: 6rpx solid #fff;
    border-right: 6rpx solid #fff;
    position: absolute;
    right: 43rpx;
}

.rorate-bot {
    transform: rotate(47deg);
    border-top: 6rpx solid #f21b7b;
    border-left: 6rpx solid #f21b7b;
    top: 43rpx;
}

.rorate-top {
    transform: rotate(-135deg);
    border-top: 6rpx solid #000;
    border-left: 6rpx solid #000;
    top: 35rpx;
}

