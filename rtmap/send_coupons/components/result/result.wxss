.dialog {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background:rgba(0,0,0,0.7);
    display: flex;
    justify-content: center;
    align-items: center;
}

.dialog_content {
    width:560rpx;
    height:432rpx;
    background:rgba(255,255,255,1);
    border-radius:8px; 
    position: relative;
}

.img {
    width:144rpx;
    height:220rpx;
    position: absolute;
    left: 50%;
    margin-left: -72rpx;
    top: -120rpx;
}
.info {
    margin-top: 180rpx;
    text-align: center;
}
.info_detail {
    height:44rpx;
    font-size:36rpx;
    font-family:PingFangSC-Medium;
    font-weight:500;
    color:rgba(51,51,51,1);
    line-height:44rpx;
}
.see_btn {
    margin-top: 50rpx;
    width:220rpx;
    height:72rpx;
    background:rgba(242,27,123,1);
    border-radius:36rpx;
    color: #fff;
    line-height: 72rpx;
}