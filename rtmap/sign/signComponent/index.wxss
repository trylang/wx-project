@keyframes ZoomIn {
    from {
        transform: scale(0.01);
    }

    to {
        transform: scale(1);
    }
}

@keyframes waveToLeft {
    0% {
        background-position-x: 0;
        background-position-y: 100%;
    }

    25% {
        background-position-x: -562.5rpx;
        background-position-y: 110%;
    }

    50% {
        background-position-x: -1125rpx;
        background-position-y: 97%;
    }

    75% {
        background-position-x: -1687.5rpx;
        background-position-y: 110%;
    }

    100% {
        background-position-x: -2250rpx;
        background-position-y: 100%;
    }
}

._signin_ .header {
    position: relative;
}

._signin_ .header .banner {
    width: 750rpx;
    height: 268rpx;
}

._signin_ .header .moon {
    width: 200rpx;
    height: 135rpx;
    background: url(data:image/png;base64,) center no-repeat;
    -webkit-background-size: contain;
    background-size: contain;
    position: absolute;
    left: 50rpx;
    top: 40rpx;
    -webkit-transform: scale(0.01);
    transform: scale(0.01);
    -webkit-animation: ZoomIn 1s both cubic-bezier(0.755, 0.005, 0.265, 1.550);
    animation: ZoomIn 1s both cubic-bezier(0.755, 0.005, 0.265, 1.550);
}

._signin_ .header .moon view {
    color: #51516a;
    width: 93%;
    font-size: 28rpx;
    font-weight: bold;
    text-align: center;
}

._signin_ .header .moon .month {
    margin-top: 15rpx;
}

._signin_ .header .moon .date {
    font-size: 50rpx;
    line-height: 45rpx;
}

._signin_ .header .sign {
    font-size: 24rpx;
    line-height: 44rpx;
    background: rgba(255, 255, 255, 0.7);
    padding-left: 20rpx;
    padding-right: 10rpx;
    border-top-left-radius: 50rpx;
    border-bottom-left-radius: 50rpx;
    position: absolute;
    right: 0;
    top: 40rpx;
}

._signin_ .header .sign view {
    font-weight: bold;
    font-size: 30rpx;
    padding: 0 5rpx;
    display: inline-block;
}

._signin_ .header .wave {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
}

._signin_ .calendar {
    width: 675rpx;
    margin: 0 auto;
    margin-top: 20rpx;
}

._signin_ .calendar .week, ._signin_ .calendar .day {
    font-size: 28rpx;
    list-style: none;
    -webkit-display: flex;
    display: flex;
}

._signin_ .calendar .week {
    color: #ccc;
    border-bottom: #ccc solid 1rpx;
    margin-bottom: 20rpx;
}

._signin_ .calendar .week view {
    line-height: 50rpx;
    text-align: center;
    -webkit-flex: 1;
    flex: 1;
}

._signin_ .calendar .day view {
    line-height: 90rpx;
    text-align: center;
    -webkit-flex: 1;
    flex: 1;
}

._signin_ .calendar .day view.disabled {
    color: #ccc;
}

._signin_ .calendar .day view.signIn {
    color: #fff;
    background: -webkit-radial-gradient(circle closest-side, #f9cb47, #faba00 80%, transparent 80%);
    background: -o-radial-gradient(circle closest-side, #f9cb47, #faba00 80%, transparent 80%);
    background: -moz-radial-gradient(circle closest-side, #f9cb47, #faba00 80%, transparent 80%);
    background: radial-gradient(circle closest-side, #f9cb47, #faba00 80%, transparent 80%);
    opacity: 0;
    filter: alpha(opcity = 0);
    -webkit-animation: fadeIn 0.3s both;
    animation: fadeIn 0.3s both;
}

._signin_ .calendar .day view.disabled.signIn {
    background: -webkit-radial-gradient(circle closest-side, #ddd, #ddd 80%, transparent 80%);
    background: -o-radial-gradient(circle closest-side, #ddd, #ddd 80%, transparent 80%);
    background: -moz-radial-gradient(circle closest-side, #ddd, #ddd 80%, transparent 80%);
    background: radial-gradient(circle closest-side, #ddd, #ddd 80%, transparent 80%);
}

._signin_ .confirm {
    font-size: 26rpx;
    padding-top: 50rpx;
    text-align: center;
}

.rules {
    padding: 50rpx 30rpx 50rpx 30rpx;
    color: #999;
    font-size: 28rpx;
}

._signin_ .confirm .rule {
    color: #999;
    max-width: 70%;
    line-height: 50rpx;
    text-align: left;
    margin: 0 auto;
}

._signin_ .wave7 {
    margin-top: -20rpx;
    opacity: 0.7;
    filter: alpha(opacity = 70);
    background: url(data:image/png;base64,) left bottom repeat-x;
    -webkit-background-size: 2250rpx 70rpx;
    background-size: 2250rpx 70rpx;
    -webkit-animation: waveToLeft 10s infinite linear;
    animation: waveToLeft 10s infinite linear;
}

._signin_ .wave10 {
    background: url(data:image/png;base64,) left bottom repeat-x;
    -webkit-background-size: 2250rpx 80rpx;
    background-size: 2250rpx 80rpx;
    -webkit-animation: waveToLeft 24s infinite linear;
    animation: waveToLeft 24s infinite linear;
}

._signin_ .button.round {
    color: #fff;
    font-size: 32rpx;
    line-height: 88rpx;
    background: #faba00;
    -webkit-box-shadow: 0 15rpx 30rpx #fddf89;
    box-shadow: 0 15rpx 30rpx #fddf89;
    padding: 0 60rpx;
    border-radius: 100rpx;
    display: inline-block;
    cursor: pointer;
}

._signin_ .button.round.success {
    background: #5c3;
    -webkit-box-shadow: 0 15rpx 30rpx #b3dba3;
    box-shadow: 0 15rpx 30rpx #b3dba3;
}

._signin_ .button.round.disabled {
    pointer-events: none;
    background: #ccc;
    -webkit-box-shadow: 0 15rpx 30rpx #d3d3d3;
    box-shadow: 0 15rpx 30rpx #d3d3d3;
}

@keyframes action_scaleOut {
    0% {
        transform: scale(1);
        opacity: 1;
    }

    100% {
        transform: scale(0.2);
        opacity: 0;
    }
}

@keyframes action_scale {
    0% {
        transform: scale(0.2);
        opacity: 0;
    }

    100% {
        transform: scale(1);
        opacity: 1;
    }
}

.show {
    visibility: visible;
    -webkit-animation: action_scale 0.5s both;
    animation: action_scale 0.5s both;
}

.hide {
    visibility: hidden;
    -webkit-animation: action_scaleOut 0.5s both;
    animation: action_scaleOut 0.5s both;
}

.wx-mask {
    position: fixed;
    z-index: 1000;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.5);
}

.wx-dialog {
    position: fixed;
    z-index: 5000;
    width: 600rpx;
    max-width: 600rpx;
    top: 40%;
    left: 10%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background-color: #fff;
    text-align: center;
    border-radius: 10px;
}

.dialog-title {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.dialog-title-content {
    height: 294rpx;
    display: flex;
    flex-direction: column;
    background-color: #fff;
    align-items: center;
}

.dialog-title-ok {
    width: 100%;
    height: 80rpx;
    background-color: #faba00;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    border-bottom-left-radius: 10px;
    border-bottom-right-radius: 10px;
}

.dialog-title-ok text {
    color: #fff;
    font-size: 36rpx;
}

.dialog-title image {
    position: absolute;
    left: 50%;
    top: 0;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    width: 280rpx;
    height: 148rpx;
}
